local library = loadstring(game.HttpGet(game, "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/0x"))()

local w1 = library:Window("FW case clickers") -- Text

w1:Toggle(
    "Auto Click",
    "frz",
    false,
    function(toggled)
local args = {
    [1] = "Click",
    [3] = 51914
}

game:GetService("ReplicatedStorage").Remotes.RemoteEvent:FireServer(unpack(args))
end)

w1:Toggle(
    "Auto Spin Wheel",
    "frz",
    false,
    function(toggled)
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SpinWheel"):InvokeServer()
end)
